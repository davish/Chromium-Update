cd ~/Downloads
wget http://build.chromium.org/f/chromium/snapshots/chromium-rel-mac/LATEST -O latest.txt
#myFile="~/Downloads/latest.txt "
myURL=""
myURL=`cat latest.txt`
echo http://build.chromium.org/f/chromium/snapshots/chromium-rel-mac/$myURL/chrome-mac.zip
wget http://build.chromium.org/f/chromium/snapshots/chromium-rel-mac/$myURL/chrome-mac.zip
unzip chrome-mac.zip
cd chrome-mac cp -r -f ~/Downloads/chrome-mac/Chromium.app ~/Applications
rm latest.txt
